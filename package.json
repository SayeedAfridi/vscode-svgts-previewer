{
    "name": "view-svg-proto",
    "displayName": "view-svg-proto",
    "description": "",
    "version": "0.0.1",
    "publisher": "Vitalii",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:xml"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
				"command": "svg.showPreview",
				"title": "%svg.preview.title%",
				"category": "Svg"
			},
            {
                "command": "svg.showPreviewToSide",
                "title": "%svg.previewSide.title%",
                "category": "Svg",
                "icon": {
                    "light": "./media/PreviewOnRightPane_16x.svg",
                    "dark": "./media/PreviewOnRightPane_16x_dark.svg"
                }
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "svg.showPreviewToSide",
                    "when": "svgActive",
                    "alt": "svg.showPreview",
                    "group": "navigation"
                }
            ],
            "commandPalette": [
                {
                    "command": "svg.showPreview",
                    "when": "svgActive",
                    "group": "navigation"
                },
                {
                    "command": "svg.showPreviewToSide",
                    "when": "svgActive",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
